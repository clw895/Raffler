@inherits raffler.Components.StartRaffleConfirmationComponent

<form>
    <div class="form-group">
        <p>Do you want to re-enter previous raffle entrants?</p>
    </div>
    <div class="form-group">
        <button type="button" class="btn btn-primary" @onclick="@StartRaffleWithEntrants">Yes</button>
        <button type="button" class="btn btn-primary" @onclick="@StartRaffleWithoutEntrants">No</button>
    </div>
</form>

